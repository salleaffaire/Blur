func int32[..] fibonacci(int32 length) {
   var int32 x;
   var int32[..] rval;
   
   rval.resize(length);
   for (x=0;x<length;{x=x+1;}) {
      if (x == 0) {
         rval[x] = x;
      }
      else {
         rval[x] = rval[x-1] + x;
      }
   }

   return rval;
}

