// Types 
// --------------------------------------------------------

// bit[] type

class bit {
   var int8 data;
}

var bit[32] a_word;
var bit[1]  just_one_bit;

class atom {
   var bit[32]      name;
   var bit[32]      size;
   var bit[32][..]  data;
}

// MP4 File
class mp4file {
   var list{atom} file;
}


